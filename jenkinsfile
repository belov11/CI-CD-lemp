pipeline {
    agent any
    environment {
        ANSIBLE_PRIVATE_KEY=credentials('jenkins-server')
    }
    stages {
        stage('Started ansible playbook') {
            steps {
                script {
                    sh 'ansible-playbook -i /etc/ansible/hosts --private-key=$ANSIBLE_PRIVATE_KEY /etc/ansible/docker-install.yml'               
                }
            }
        }
    }
}
