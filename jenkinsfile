pipeline {
    agent any
    environment {
        ANSIBLE_PRIVATE_KEY=credentials('web-server-private-key')
    }
    stages {
        stage('Started ansible playbook') {
            steps {
                script {
                    sh 'cd /etc/ansible/'
                    sh 'ansible-playbook -i inventory/hosts --private-key=$ANSIBLE_PRIVATE_KEY playbooks/docker-install.yml'

                    
                }
            }
        }
    }
}
